	@${MAKE} --no-print-directory -C dependencies -f wxXRC.make config=$(wxXRC_config)
endif

zlib-ng:
ifneq (,$(zlib_ng_config))
	@echo "==== Building zlib-ng ($(zlib_ng_config)) ===="
	@${MAKE} --no-print-directory -C dependencies -f zlib-ng.make config=$(zlib_ng_config)
endif

Esprit:
ifneq (,$(Esprit_config))
	@echo "==== Building Esprit ($(Esprit_config)) ===="
	@${MAKE} --no-print-directory -C frameworks -f Esprit.make config=$(Esprit_config)
endif

