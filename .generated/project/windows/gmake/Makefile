clean:
	@${MAKE} --no-print-directory -C dependencies -f assimp.make clean
	@${MAKE} --no-print-directory -C dependencies -f EASTL.make clean
	@${MAKE} --no-print-directory -C dependencies -f easy_profiler.make clean
	@${MAKE} --no-print-directory -C dependencies -f expat.make clean
	@${MAKE} --no-print-directory -C dependencies -f libjpeg.make clean
	@${MAKE} --no-print-directory -C dependencies -f libpng.make clean
	@${MAKE} --no-print-directory -C dependencies -f libtiff.make clean
	@${MAKE} --no-print-directory -C dependencies -f libuv.make clean
	@${MAKE} --no-print-directory -C dependencies -f minizip.make clean
	@${MAKE} --no-print-directory -C dependencies -f mpack.make clean
	@${MAKE} --no-print-directory -C dependencies -f rpmalloc.make clean
	@${MAKE} --no-print-directory -C dependencies -f wxBase.make clean
	@${MAKE} --no-print-directory -C dependencies -f wxCore.make clean
	@${MAKE} --no-print-directory -C dependencies -f wxXML.make clean
	@${MAKE} --no-print-directory -C dependencies -f wxAdv.make clean
	@${MAKE} --no-print-directory -C dependencies -f wxAui.make clean
	@${MAKE} --no-print-directory -C dependencies -f wxHTML.make clean
	@${MAKE} --no-print-directory -C dependencies -f wxMedia.make clean
	@${MAKE} --no-print-directory -C dependencies -f wxPropGrid.make clean
	@${MAKE} --no-print-directory -C dependencies -f wxQA.make clean
	@${MAKE} --no-print-directory -C dependencies -f wxRegex.make clean
	@${MAKE} --no-print-directory -C dependencies -f wxRibbon.make clean
	@${MAKE} --no-print-directory -C dependencies -f wxRichText.make clean
	@${MAKE} --no-print-directory -C dependencies -f wxWebview.make clean
	@${MAKE} --no-print-directory -C dependencies -f wxXRC.make clean
	@${MAKE} --no-print-directory -C dependencies -f zlib-ng.make clean
	@${MAKE} --no-print-directory -C frameworks -f Esprit.make clean
	@${MAKE} --no-print-directory -C frameworks -f Gaff.make clean
	@${MAKE} --no-print-directory -C frameworks -f Gleam.make clean
	@${MAKE} --no-print-directory -C engine -f Editor.make clean
	@${MAKE} --no-print-directory -C engine -f Editor_App.make clean
	@${MAKE} --no-print-directory -C engine -f Engine.make clean
	@${MAKE} --no-print-directory -C engine -f Game_App.make clean
	@${MAKE} --no-print-directory -C engine -f Memory.make clean
	@${MAKE} --no-print-directory -C modules -f ECS.make clean
	@${MAKE} --no-print-directory -C modules -f ECSModule.make clean
	@${MAKE} --no-print-directory -C modules -f EditorBasic.make clean
	@${MAKE} --no-print-directory -C modules -f EditorBasicModule.make clean
	@${MAKE} --no-print-directory -C modules -f EditorMainLoop.make clean
	@${MAKE} --no-print-directory -C modules -f EditorMainLoopModule.make clean
	@${MAKE} --no-print-directory -C modules -f Entity.make clean
	@${MAKE} --no-print-directory -C modules -f EntityModule.make clean
	@${MAKE} --no-print-directory -C modules -f GraphicsDirect3D11.make clean
	@${MAKE} --no-print-directory -C modules -f GraphicsDirect3D11Module.make clean
	@${MAKE} --no-print-directory -C modules -f MainLoop.make clean
	@${MAKE} --no-print-directory -C modules -f MainLoopModule.make clean
	@${MAKE} --no-print-directory -C modules -f Resource.make clean
	@${MAKE} --no-print-directory -C modules -f ResourceModule.make clean
	@${MAKE} --no-print-directory -C tests -f AllocatorTest.make clean
	@${MAKE} --no-print-directory -C tests -f ReflectionTest.make clean

