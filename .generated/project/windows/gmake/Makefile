Editor:
ifneq (,$(Editor_config))
	@echo "==== Building Editor ($(Editor_config)) ===="
	@${MAKE} --no-print-directory -C engine -f Editor.make config=$(Editor_config)
endif

Editor_App: Engine Memory Editor Gaff EASTL libjpeg libpng zlib-ng wxBase wxCore wxAui wxAdv
ifneq (,$(Editor_App_config))
	@echo "==== Building Editor_App ($(Editor_App_config)) ===="
	@${MAKE} --no-print-directory -C engine -f Editor_App.make config=$(Editor_App_config)
endif

Engine:
ifneq (,$(Engine_config))
	@echo "==== Building Engine ($(Engine_config)) ===="
	@${MAKE} --no-print-directory -C engine -f Engine.make config=$(Engine_config)
endif

Game_App: Engine Gaff Memory EASTL
ifneq (,$(Game_App_config))
	@echo "==== Building Game_App ($(Game_App_config)) ===="
	@${MAKE} --no-print-directory -C engine -f Game_App.make config=$(Game_App_config)
endif

Memory: Gaff EASTL rpmalloc
ifneq (,$(Memory_config))
	@echo "==== Building Memory ($(Memory_config)) ===="
	@${MAKE} --no-print-directory -C engine -f Memory.make config=$(Memory_config)
endif

ECS:
ifneq (,$(ECS_config))
	@echo "==== Building ECS ($(ECS_config)) ===="
	@${MAKE} --no-print-directory -C modules -f ECS.make config=$(ECS_config)
endif

